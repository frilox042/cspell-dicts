{
    "version": "0.2",
    "language": "en",
    "files": [
        "*.{md,txt,yaml,yml}",
        ".github/**/*.{md,txt,yaml,yml}",
        "dictionaries/**/*.{md,txt,yaml,yml,js,tex}",
        "LICENSE"
    ],
    "words": [
        "ALLCAPS",
        "LGPL",
        "appname",
        "autobuild",
        "codeql",
        "configstore",
        "crowdsourcing",
        "dependabot",
        "django",
        "django's",
        "issuehunt",
        "liberapay",
        "mdash",
        "otechie",
        "patreon",
        "streetsidesoftware",
        "tidelift",
        "z"
    ],
    "flagWords": [],
    "maxNumberOfProblems": 1000,
    "ignorePaths": [
        ".vscode/**",
        "**/.git/**",
        "*.aff",
        "*.dic",
        "*.log",
        "*.gz",
        "*.lock",
        "generator-cspell-dicts/generators/app/templates/**",
        "dictionaries/*/src/**",
        "node_modules"
    ],
    "import": [],
    "overrides": [
        {
            "filename": "**/nl_NL/**",
            "language": "en,nl"
        },
        {
            "filename": "**/ru_RU/samples/**",
            "language": "ru,en"
        },
        {
            "filename": "**/sv/**",
            "language": "en,sv"
        }
    ]
}
